img.profile-arrow{
  margin-bottom: 1px;
  margin-left: 5px;
}
.profile-avatar {
  width: 25px;
  height: 25px;
  float: left;
  margin-top: -5px;
  margin-right: 7px;
  background-color: #ccc;
}
#dropdown-box {
  position: absolute;
  top: 25px;
  right: 0px;
  z-Index: 99;
  padding: 10px;
  background-color: white;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
}
tr#dropdown-text {
  color: #ccc;
  border-bottom: 1px solid #ccc;
}
tr#dropdown-text-last {
  color: #ccc;
}
td#dropdown-text {
  font-weight: 700;
  font-size: 12px;
  color: #666666;
  padding-top: 3px;
  padding-bottom: 3px;
  padding-right: 30px;
}

table#menu {
  position: relative;
  float: right;
  border-collapse: collapse;
  margin-top: 20px;
}

td#menu_teach, td#menu_suggest, td#menu_business {
  border: 1px solid #d0cdcd;
  height: 60px;
  vertical-align: bottom;
  text-align: center;
  vertical-align: middle;
}
td#menu_learn, td#menu_faq {
  border-top: 1px solid #d0cdcd;
  border-bottom: 1px solid #d0cdcd;
  border-left: 0px;
  border-right: 0px;
  height: 60px;
  vertical-align: bottom;
  text-align: center;
  vertical-align: middle;
}
td#menu_learn {
  width: 180px;
}
td#menu_teach {
  width: 180px;
}
td#menu_suggest {
  width: 180px;
}
td#menu_business {
  width: 180px;
}
td#menu_faq {
  width: 67px;
}
a#menu {
  color: #8f8f8f;
  text-decoration: none;
}

a#menu:visited {
  color: #8f8f8f;
  text-decoration: none;
}
a#menu:hover {
  text-decoration: none;
  color: #ff7733;
}

a#menu-active{
  text-decoration: none;
  color: #ff7733;
}

a#menu-inactive{
  text-decoration: none;

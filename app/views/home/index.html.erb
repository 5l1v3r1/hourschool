 <!DOCTYPE html>
<html>
<head>
  <title>Greendoors campus</title>

  <link rel="icon"
        type="image/png"
        href="/images/favicon.png">

  <%= stylesheet_link_tag 'screen.css', :media => 'screen, projection' %>

  <%= javascript_include_tag :all %>

  <%= csrf_meta_tag %>

  <script type="text/javascript" src="http://use.typekit.com/xjt1fgx.js"></script>
  <script type="text/javascript">try{Typekit.load();}catch(e){}</script>

  <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.13/jquery-ui.min.js"></script>
  <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js"></script>



  <script type="text/javascript">

    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-22725750-1']);
    _gaq.push(['_trackPageview']);

    (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();

  </script>


  <script>
    $(document).ready(function() {
      $("h1#homepage_headline").lettering('words');
    });
  </script>
  <style type="text/css">
       a:visited {
      text-decoration: underline;
      color: #4F4E4E;
    }

    /* line 35, ../../app/assets/stylesheets/components/homepage.scss */
    a:active {
      text-decoration: underline;
      color: #4F4E4E;
    }
</style>
</head>
<body id="hasbanner">


    <div class="container_16">

      <div class="page">
        <div class="page-content">

        <div class="page-section-float-left">
          <ul class="sub-nav">
            <li class="sub-nav-inactive">
              <a class="sub-nav-inactive" href="/users/sign_up">Register</a>
            </li>
            <li class="sub-nav-active">
              <a class="sub-nav-active" href="/users/sign_in">Sign in</a>
            </li>
          </ul>
        </div>

        <%- if resource.present? && resource.email.present? -%>
          <div class="page-section-float-left">
              <div class="error-block"><div class="form-error">* Email or Password is Incorrect</div></div>
          </div>
        <%- end -%>

          <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
              <div class="form-question">
              <%= f.label :email, :class => "login-text" %><br />
              <%= f.email_field :email, :class => "form-short" %>
              </div>

              <div class="form-question">
              <%= f.label :password, :class => "login-text" %><br />
                <%= f.password_field :password , :class => "form-short"%>
              </div>

              <div class="page-section-float-left">
                <% if devise_mapping.rememberable? -%>
                <%= f.check_box :remember_me %>
                <%= f.label :remember_me, :class => "login-text" %>
                <% end -%>
              </div>

              <div class="page-section-float-left">
                <div><%= f.submit "Sign in", :class => "loginButton" %></div>
              </div>
          <% end %>


          <div class="page-section-float-left">
            <%= render :partial => "devise/shared/links" %>
          </div>




        </div>
      </div>

    
    </div><!-- /end .container16 -->



<div class="homepage_middle_bg2">
    <%= render :partial => 'happenings' %>
</div>

<div class="bottom_bg">
  <%= render :partial => 'layouts/application/footer' %>
</div><!-- /end .bottom_bg -->
